{
  "name": "queen-collection",
  "version": "1.0.0",
  "description": "express application to use with my queen collection mysql database",
  "main": "index.ts",
  "scripts": {
    "start": "ts-node ./index.ts",
    "watch": "nodemon -e ts -w ./src -x npm run start",
    "lint": "tslint -p tsconfig.json",
    "ps": "ts-node ./postgres.ts",
    "make-seed": "knex seed:make",
    "seed": "knex seed:run",
    "make-migrate": "knex migrate:make",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback"
  },
  "author": "Aleksandr Pasharin",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "csvtojson": "^2.0.10",
    "dotenv": "^8.1.0",
    "express": "^4.16.3",
    "knex": "^0.15.0",
    "mysql": "^2.15.0",
    "pg": "^7.12.1",
    "promise-mysql": "^3.2.1",
    "ts-node": "^6.0.2",
    "typescript": "^2.8.3"
  },
  "devDependencies": {
    "@types/express": "^4.11.1",
    "@types/node": "^10.0.4",
    "nodemon": "^1.17.3"
  }
}
